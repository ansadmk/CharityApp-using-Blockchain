pragma solidity >=0.4.22 <0.9.0;

contract charityinfo{
uint public v_count=0;
    mapping(uint => charity) public vehs;
    struct charity{        
       uint id;
	  uint bid;
        uint Lid;
        uint reqid;
	  string Details;
        string date;
        string Status;
        string image;
    }
    function add_info(uint _bid,uint _Lid,uint _reqid,string memory _Details,string memory _date, string memory _Status, string memory _image)public{
        v_count++;
     vehs[v_count]=charity(v_count,_bid,_Lid,_reqid,_Details,_date,_Status,_image);
    }


    
}
